{
  "version": 2,
  "env": {
    "DATABASE_URL": "@hashi_bank_database_url",
    "NEXTAUTH_URL": "https://hashi-bank.vercel.app",
    "NEXTAUTH_SECRET": "@hashi_bank_nextauth_secret",
    "NODE_ENV": "production"
  },
  "buildCommand": "npm run vercel-build",
  "outputDirectory": ".next",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "framework": "nextjs",
  "functions": {
    "app/**/*.tsx": {
      "maxDuration": 30
    },
    "app/**/*.ts": {
      "maxDuration": 30
    }
  }
}